generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}


model Calendar {
    id                  String @default(cuid()) @id
}

model User {
    id                  String @default(cuid()) @id
    username            String @unique
    password            String
    registrationDate    DateTime @default(now())
}